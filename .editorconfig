root = true

[*]
charset = utf-8
end_of_line = unset
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.js]
end_of_line = lf

[*Formatter.js]
end_of_line = lf
indent_style = tab
indent_size = 1
tab_width = 2

[*.yml]
indent_size = 2

[{package.json,package-lock.json}]
end_of_line = lf
